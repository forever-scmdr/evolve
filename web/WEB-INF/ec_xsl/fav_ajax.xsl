<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE stylesheet [<!ENTITY nbsp "&#160;"><!ENTITY copy "&#x000A9;" >]><xsl:stylesheet	xmlns:xsl="http://www.w3.org/1999/XSL/Transform"	xmlns="http://www.w3.org/1999/xhtml"	xmlns:f="f:f"	version="2.0">	<xsl:output method="html" encoding="UTF-8" media-type="text/html" indent="yes" omit-xml-declaration="yes"/>	<xsl:variable name="prod" select="page/product"/>	<xsl:template match="/">		<div>		<xsl:if test="$prod">			<div class="result" id="fav_ajax">				<a href="{page/fav_link}"  rel="nofollow"><i class="fas fa-star"/>Избранное (<xsl:value-of select="count($prod)"/>)</a>			</div>			<xsl:for-each select="$prod">				<div class="result" id="fav_list_{@id}"  rel="nofollow">					<a href="{//page/fav_link}" class="icon-link device__action-link device__action-link_active"><i class="fas fa-star"></i><span>избранное</span></a>				</div>				<span id="fav_list_a_{@id}" class="result">					<a href="{//page/fav_link}" class="add__favourite active" rel="nofollow">						<svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">							<path d="M12 2L15.09 8.26L22 9.27L17 14.14L18.18 21.02L12 17.77L5.82 21.02L7 14.14L2 9.27L8.91 8.26L12 2Z" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>						</svg>					</a>				</span>			</xsl:for-each>		</xsl:if>		</div>		<xsl:if test="not($prod)">			<div class="result" id="fav_ajax">				<a><i class="fas fa-star"/>Избранное</a>			</div>		</xsl:if>	</xsl:template></xsl:stylesheet>